{
  "name": "yogicat-ds",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Dahye Oh",
  "private": true,
  "license": "MIT",
  "scripts": {
    "boot": "yarn --frozen-lockfile && yarn bootstrap",
    "bootstrap": "yarn lerna:bootstrap",
    "lerna:bootstrap": "lerna bootstrap --use-workspaces",
    "build": "lerna run build --stream",
    "build:packages": "yarn build",
    "changeset": "changeset",
    "release": "changeset publish",
    "clean": "lerna clean --yes",
    "clean:dist": "lerna exec -- rimraf dist",
    "clean:packages": "yarn clean && yarn clean:dist",
    "commitlint": "commitlint",
    "commit": "cz",
    "husky": "husky"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.1",
    "lerna": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yogicat/yogicat-ds.git"
  },
  "dependencies": {
    "@changesets/cli": "^2.16.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
